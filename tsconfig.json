{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "CommonJS",
    "target": "ES6",
    "baseUrl": ".",
    "paths": {
      "@/controllers/*": ["src/controllers/*"],
      "@/models/*": ["src/modals/*"],
      "@/routes/*": ["src/routes/*"],
      "@/schemas/*": ["src/sections/*"],
      "@/middlewares/*": ["src/middlewares/*"],
      "@/services/*": ["src/services/*"],
      "@/types/*": ["src/types/*"]
    }
  }
}
